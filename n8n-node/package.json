    "url": "git+https://github.com/tehw0lf/n8n-nodes-unix-socket-bridge.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "unix-socket",
    "ipc",
    "system-control",
    "automation",
    "playerctl",
    "docker",
    "system-monitoring",
    "socket",
    "bridge",
    "n8n"
  ],
  "author": "tehw0lf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehw0lf/n8n-nodes-unix-socket-bridge/issues"
  },
  "homepage": "https://github.com/tehw0lf/n8n-nodes-unix-socket-bridge#readme",
